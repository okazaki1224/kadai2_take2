<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    
  </tbody>
</table>

<h2>New book</h2>

  <%= form_with model:@book,url:'books',method: :post do |f| %>
  <h4>Title</h4>
  <%= f.text_field:Title %>
  <h4>Body</h4>
  <%= f.text_area:Body %></br>

  <%= f.submit 'Create Books' %>
  <% end %>
